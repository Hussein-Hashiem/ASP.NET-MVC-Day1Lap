@using MVCDay1.DAL.Entities
@model List<Employee>

<table class="table">
  <thead>
	<tr>
	  <th scope="col">#</th>
	  <th scope="col">Name</th>
	  <th scope="col">Age</th>
	</tr>
  </thead>
  <tbody class="table-group-divider">
		@foreach (var i in Model)
		{
			<tr>
				  <th scope="row">@i.ID</th>
				  <td>@i.Name</td>
				  <td>@i.Age</td>
			</tr>
		}
  </tbody>
</table>